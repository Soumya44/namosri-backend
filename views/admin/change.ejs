<!DOCTYPE html>
  <html>
    <head>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/libs/materialize.min.css"  media="screen,projection"/>

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <link rel="stylesheet" href="/css/admin/update.css">
      <%- include partials/loadercss %>
    </head>

    <body>
        
        <%- include partials/loader %>
        <div class="main container">
          <h3 class="center-align">Change Your Info</h3>
          <form id="changeForm">
            <div class="input-field">
              <input type="text" id="oldUsername">
              <label for="oldUsername">Old Username</label>
            </div>
            <div class="input-field">
              <input type="text" id="newUsername">
              <label for="newUsername">New Username</label>
            </div>
            <div class="input-field">
              <input type="password" id="oldPassword">
              <label for="oldPassword">Old Password</label>
            </div>
            <div class="input-field">
              <input type="password" id="newPassword">
              <label for="newPassword">New Password</label>
            </div>
            <div class="input-field">
              <input type="password" id="retypedPassword">
              <label for="retypedPassword">Retype Password</label>
            </div>
            <input type="submit" class="waves-effect waves-light btn" value="Change Crediantials">
          </form>

          <div class="info"></div>
        </div>
        <%- include partials/errors %>
      <!--JavaScript at end of body for optimized loading-->
      <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
      <script type="text/javascript" src="/js/libs/materialize.min.js"></script>
      <script src="/js/admin/change.js"></script>
    </body>
  </html>