<!DOCTYPE html>
  <html>
    <head>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/libs/materialize.min.css"  media="screen,projection"/>

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <title>Admin main page</title>
      <link rel="stylesheet" href="/css/admin/index.css">
      <%- include partials/loadercss %>
    </head>

    <body>
      
        <%- include partials/loader %>

        <div class="main container">
            <h3 class="center-align">Dashboard</h3>
            <ul class="collapsible popout">
              <li>
                <div class="collapsible-header">Manipulate User</div>
                <div class="collapsible-body">
                  <div class="clickMe" data-url="/user/signup">Add User</div>
                  <div class="clickMe" data-url="/user/delete">Delete User</div>
                  <div class="clickMe" data-url="/user/changeInfo">Update Your Info</div>
                </div>
              </li>
              <li>
                <div class="collapsible-header">Manipulate Product</div>
                <div class="collapsible-body">
                  <div class="clickMe" data-url="/add/products">Add Product</div>
                  <div class="clickMe" data-url="/delete/products">Delete Product</div>
                  <div class="clickMe" data-url="/update/products">Update Product Info</div>
                </div>
              </li>
              <li>
                <div class="collapsible-header">Manipulate Recipe</div>
                <div class="collapsible-body">
                  <div class="clickMe" data-url="/add/recipe">Add Recipes</div>
                  <div class="clickMe" data-url="/delete/recipe">Delete Recipes</div>
                  <div class="clickMe" data-url="/update/recipe">Update Recipes Info</div>
                </div>
              </li>
            </ul>
        </div>
        
        <%- include partials/errors %>

      <!--JavaScript at end of body for optimized loading-->
      <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
      <script type="text/javascript" src="/js/libs/materialize.min.js"></script>
      <script src="/js/admin/index.js"></script>
    </body>
  </html>