<!DOCTYPE html>
  <html>
    <head>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/libs/materialize.min.css"  media="screen,projection"/>

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <title>Login page</title>
      <link rel="stylesheet" href="/css/admin/login.css">
      <%- include partials/loadercss %>
    </head>

    <body>
        <%- include partials/loader %>
        <br><br><br>
    <div class="row">
    <div class="col s12 m3"></div>
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="main container ">
            <h3 class="center-align">Login Form</h3>
            <form id="loginForm">
                <div class="input-field">
                    <input type="text" id="username" />
                    <label for="username">Username</label>
                </div>
                <div class="input-field">
                    <input type="password" id="password" />
                    <label for="password">Password</label>
                </div>
                <input class="waves-effect waves-light btn" type="submit" value="Login"/>
            </form>
        </div>
      </div>
    </div>
  </div>


        <%- include partials/errors %>

      <!--JavaScript at end of body for optimized loading-->
      <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
      <script type="text/javascript" src="/js/libs/materialize.min.js"></script>
      <script src="/js/admin/login.js"></script>
    </body>
  </html>